{
    "name": "turbodev/rp_laravel_bridge",
    "description": "Bridge between rp_contracts and Laravel",
    "repositories":[
        {
            "type": "vcs",
            "url": "git@github.com:almatyartem/rp_contracts.git"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "turbodev/rp_contracts": "dev-master",
        "illuminate/support": "*",
        "ext-json": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "src"
        ],
        "psr-4": {
            "RpLaravelBridge\\": "src/"
        }
    }
}
